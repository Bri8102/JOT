
  <h2> Welcome, <%= current_user.email if !current_user.nil? %></h2>
    
  <h4>Here are all Lists</h4>

  <% @lists.each do |list| %>
    <div class="index_row clearfix">
      <h2 class="todo_list_title"><%= link_to list.title, list %></h2>
      <p class="todo_list_sub_title"><%= list.description %></p>
    </div>
    <% end %>

    <div class="links">
      <%= link_to "Create List", new_list_path %>
    </div>

    
 
